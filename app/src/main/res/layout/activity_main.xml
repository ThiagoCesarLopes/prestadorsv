<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/scrowView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:fillViewport="true"
        android:gravity="top"
        tools:context=".MainActivity" >
        <TabHost
        android:id="@+id/tabHost"
        android:layout_width="385dp"
        android:layout_height="515dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="#BEBEBE"
        android:fadeScrollbars="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TabWidget
                android:id="@android:id/tabs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:background="@android:color/background_light"></TabWidget>

            <FrameLayout
                android:id="@android:id/tabcontent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_weight="1">

                <LinearLayout
                    android:id="@+id/HOME"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/Principal2"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/btnserviços_pintor"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_pintor"
                            android:onClick="onclickPintor"
                            android:text="Pintor" />

                        <Button
                            android:id="@+id/btnservicos_manicure"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_manicure"
                            android:onClick="onclickManicure"
                            android:text="Manicure" />

                        <Button
                            android:id="@+id/btnserviços_cabeleleiro"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_beauty"
                            android:onClick="onclickCabeleleiro"
                            android:text="Cabeleleiros" />

                        <Button
                            android:id="@+id/btnservicos_encanador"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_people"
                            android:onClick="onclickEncanador"
                            android:text="Encanador" />

                        <Button
                            android:id="@+id/btnserviços_eletricista"
                            android:layout_width="match_parent"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_pintor"
                            android:onClick="onclickEletricista"
                            android:text="Eletricista" />

                        <Button
                            android:id="@+id/btnserviços_baba"
                            android:layout_width="match_parent"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_pintor"
                            android:onClick="onclickBaba"
                            android:text="Babá" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/Principal3"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/btnservicos_roupa2"
                            android:layout_width="match_parent"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_costureira"
                            android:onClick="onclickCostureira"
                            android:text="Costureiras" />

                        <Button
                            android:id="@+id/chaveiro"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_veiculo"
                            android:onClick="onclickChaveiro"
                            android:text="Chaveiros" />

                        <Button
                            android:id="@+id/btnservicos_gerais"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_housekeeper"
                            android:onClick="onclickDiarista"
                            android:text="Diaristas" />

                        <Button
                            android:id="@+id/btnservicos_pedreiro"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_pedreiro"
                            android:onClick="onclickPedreiro"
                            android:text="Pedreiros" />

                        <Button
                            android:id="@+id/btnservicos_marcineiro"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_costureira"
                            android:onClick="onclickMarcineiro"
                            android:text="Marcineiro" />

                        <Button
                            android:id="@+id/btnserviços_vidraceiro"
                            android:layout_width="match_parent"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_pintor"
                            android:onClick="onclickVidraceiro"
                            android:text="Vidraceiro" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/Principal4"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/btnserviços_Cozinheiro"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_cooking"
                            android:onClick="onclickCozinheiro"
                            android:text="Cozinheiro" />

                        <Button
                            android:id="@+id/btnservicos_Celular"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_celular"
                            android:onClick="onclickCelulare"
                            android:text="Celulares" />

                        <Button
                            android:id="@+id/btnserviços_Fisioterapeuta"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_physiotherapy"
                            android:onClick="onclickFisioterapeuta"
                            android:text="Fisioterapeuta" />

                        <Button
                            android:id="@+id/btnservicos_esteticista"
                            android:layout_width="120dp"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_woman_face"
                            android:onClick="onclickEsteticista"
                            android:text="Esteticista" />

                        <Button
                            android:id="@+id/btnserviços_barman"
                            android:layout_width="match_parent"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_barman"
                            android:onClick="onclickBarman"
                            android:text="BarMan" />

                        <Button
                            android:id="@+id/btnserviços_borracheiro"
                            android:layout_width="match_parent"
                            android:layout_height="90dp"
                            android:background="#D3D3D3"
                            android:drawableTop="@drawable/ic_servico"
                            android:onClick="onclickBorracheiro"
                            android:text="Borracheiro" />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/Prestador"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:text="Cadastro do Prestador"
                    android:orientation="vertical">



                </LinearLayout>
                <LinearLayout
                    android:id="@+id/Contato"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                             <TextView
                                android:id="@+id/TextViewTitle"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/colorPrimary"
                                android:layout_marginTop="40dp"
                                android:text="Suporte ou Feedback?"
                                android:textSize="10pt"></TextView>


                            <EditText
                                android:id="@+id/EditTextName"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:hint="@string/feedbackname"
                                android:inputType="textPersonName"></EditText>

                            <EditText
                                android:id="@+id/EditTextEmail"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:hint="@string/feedbackemail"
                                android:inputType="textEmailAddress"></EditText>

                            <Spinner
                                android:id="@+id/SpinnerFeedbackType"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:entries="@array/feedbacktypelist"
                                android:prompt="@string/feedbacktype"></Spinner>

                            <EditText
                                android:id="@+id/EditTextFeedbackBody"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:hint="@string/feedbackbody"
                                android:inputType="textMultiLine"
                                android:lines="5"></EditText>

                            <CheckBox
                                android:id="@+id/CheckBoxResponse"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/feedbackresponse"></CheckBox>

                            <Button
                                android:id="@+id/ButtonSendFeedback"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="30dp"
                                android:background="@color/colorPrimary"
                                android:onClick="sendFeedback"
                                android:text="@string/feedbackbutton"
                                android:theme="@style/Base.Widget.AppCompat.ButtonBar.AlertDialog"></Button>


                </LinearLayout>

            </FrameLayout>
        </LinearLayout>
    </TabHost>
    </ScrollView>
</android.support.constraint.ConstraintLayout>
